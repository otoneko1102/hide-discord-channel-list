{
  "manifest_version": 3,
  "default_locale": "en",
  "name": "__MSG_Name__",
  "version": "2.0.0",
  "description": "__MSG_Description__",
  "content_scripts": [
    {
      "matches": [
        "https://discord.com/*",
        "https://canary.discord.com/*",
        "https://ptb.discord.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/16x16.png"
  }
}
