{
  "manifest_version": 3,
  "name": "Hide Discord Channel List",
  "version": "1.0.2",
  "description": "Discordのチャンネルリストの表示/非表示を切り替えるボタンを追加します",
  "content_scripts": [
    {
      "matches": [
        "https://discord.com/*",
        "https://canary.discord.com/*",
        "https://ptb.discord.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/16x16.png"
  }
}
